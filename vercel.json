{
  "regions": ["sin1"],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "www.cambroos.com" }
      ],
      "destination": "https://cambroos.com/:1",
      "permanent": true
    },
    {
      "source": "http://(.*)",
      "destination": "https://cambroos.com",
      "permanent": true
    }
  ]
}
